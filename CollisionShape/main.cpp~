#include "CollisionShapeRect.h"
#include "CollisionShapeCircle.h"
#include <iostream>
using namespace std;

int main()
{
	CollisionShapeCircle circle(1,1,1);
	CollisionShapeRect rect(4.0,4.0,1.0,.5);
	CollisionShape * a = (CollisionShape *)&rect;
	
	if(circle.isOverlap(a))
	{
		cout<<"jaspal"<<endl;
	}
	cout<<"singh"<<endl;
return 0;
}
